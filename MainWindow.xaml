<Window x:Class="WeChatDecryptor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeChatDecryptor"
        mc:Ignorable="d" Width="400" Height="150"
        Padding="20"
        Title="MainWindow">
    <StackPanel Orientation="Vertical">
        <Button Click="Button_Click" Margin="8" Padding="4">OPEN</Button>
        <TextBox x:Name="Tbx_Key" Padding="4" Margin="8 0"></TextBox>
    </StackPanel>
</Window>
